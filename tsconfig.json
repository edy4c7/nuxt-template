{
  "extends": "./node_modules/@nuxt/typescript/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",                       /* Base directory to resolve non-absolute module names. */
    "resolveJsonModule": true,
    "types": [
      "@types/node",
      "@nuxt/vue-app"
    ],                           /* Type declaration files to be included in compilation. */
  },
  "exclude": [
    "*.config.js",
    "./modules/config"
  ]
}
